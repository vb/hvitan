---
import Navigation from './Navigation.astro';

export interface Props {
  title: string;
}
const { title } = Astro.props;
---

<div>
  <a href="/" class="logo-link">
    <img class="logo" src="/logo.jpg" alt="Kollektivet Hvitan logo" />
  </a>
  <Navigation title={title} />
</div>
<style>
  .logo-link {
    margin: 1em 0px;
    display: block;
  }

  .logo {
    width: 100%;
    max-width: 100%;
    display: block;
    height: auto;
  }
</style>
